{
  "name": "geotiff-read-bbox",
  "version": "1.1.0",
  "description": "Read Pixel Values from a GeoTIFF within a Bounding Box in almost any Projection",
  "type": "module",
  "main": "index.cjs",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "index.mjs",
    "index.cjs"
  ],
  "scripts": {
    "build": "npx babel --plugins @babel/plugin-transform-modules-commonjs --plugins @babel/plugin-proposal-export-namespace-from index.mjs --out-file index.cjs && echo '\nmodule.exports = exports.default; module.exports.default = exports.default;' >> index.cjs",
    "format": "npx prettier --arrow-parens=avoid --print-width=120 --trailing-comma=none --write *js",
    "prepublish": "npm run format && npm run build && npm test",
    "setup": "cd data && ./setup.sh",
    "test": "node test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeoTIFF/geotiff-read-bbox.git"
  },
  "keywords": [
    "bbox",
    "boundingbox",
    "clip",
    "geotiff",
    "imagery",
    "project",
    "proj4",
    "satellite",
    "raster",
    "read"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/GeoTIFF/geotiff-read-bbox/issues"
  },
  "homepage": "https://github.com/GeoTIFF/geotiff-read-bbox#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "flug": "^2.3.1",
    "geotiff": "^2.0.5"
  },
  "dependencies": {
    "geotiff-epsg-code": "^0.0.2",
    "reproject-bbox": "^0.4.1",
    "snap-bbox": "^0.3.0"
  }
}
